package uk.ac.cam.oda22.tests;

import java.awt.geom.Point2D;
import java.util.LinkedList;
import java.util.List;

import org.testng.annotations.BeforeClass;

import uk.ac.cam.oda22.core.astar.AStarNode;

/**
 * @author Oliver
 *
 */
public class TestAStar {

	private List<AStarNode> nodes;
	
	@BeforeClass
	public void oneTimeSetUp() {
		AStarNode node1 = new AStarNode(new Point2D.Double(5, 6.5));
		AStarNode node1 = new AStarNode(new Point2D.Double(2.2, 20.5));
		AStarNode node1 = new AStarNode(new Point2D.Double(10.8, 12.1));
		AStarNode node1 = new AStarNode(new Point2D.Double(1, 25));
		AStarNode node1 = new AStarNode(new Point2D.Double(6.9, 25));
		AStarNode node1 = new AStarNode(new Point2D.Double(18, 20));
		AStarNode node1 = new AStarNode(new Point2D.Double(6.7, 33.2));
		
		this.nodes = new LinkedList<AStarNode>();
		this.nodes.add(node1);
	}
	
}
