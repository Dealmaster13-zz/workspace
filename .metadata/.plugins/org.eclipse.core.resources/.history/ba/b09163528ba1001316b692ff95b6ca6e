package uk.ac.cam.oda22.coverage.simple;

import java.util.ArrayList;
import java.util.List;

/**
 * @author Oliver
 *
 */
public class SimpleCoverageResult {

	List<RoomCellIndex> cellList;
	
	public SimpleCoverageResult(SimpleCoverageRouteNode finalNode) {
		List<RoomCellIndex> reverseCellList = new ArrayList<RoomCellIndex>();
		
		SimpleCoverageRouteNode currentNode = finalNode;
		
		while (currentNode != null) {
			reverseCellList.add(currentNode);
			
			currentNode = currentNode.previousNode;
		}
	}
	
}
