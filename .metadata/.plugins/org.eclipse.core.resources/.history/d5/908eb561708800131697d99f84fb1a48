package uk.ac.cam.oda22.pathplanning;

import java.util.List;

import uk.ac.cam.oda22.core.robots.actions.IRobotAction;
import uk.ac.cam.oda22.core.tethers.TetherConfiguration;

/**
 * @author Oliver
 *
 */
public class PathPlanningResult {

	public final List<IRobotAction> actions;
	
	public final Path path;
	
	public final TetherConfiguration tc;
	
	public PathPlanningResult(List<IRobotAction> actions, Path path, TetherConfiguration tc) {
		this.actions = actions;
		this.path = path;
		this.tc = tc;
	}
	
}
