package uk.ac.cam.oda22.core.robots.actions.simple;

/**
 * @author Oliver
 * 
 */
public enum SimpleMoveDirection {

	UP,

	RIGHT,

	DOWN,

	LEFT;

	@Override
	public String toString() {
		switch (this) {
		case UP:
			return "up";
		case RIGHT:
			return "right";
		case DOWN:
			return "down";
		case LEFT:
			return "left";
		default:
			return new IllegalArgumentException();
		}
	}

}
